{
  "name": "@pixelsprout/chorus-vue",
  "version": "0.2.1",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./components/*": "./src/components/*",
    "./providers/*": "./src/providers/*",
    "./src/providers/ChorusProvider.vue": "./src/providers/ChorusProvider.vue",
    "./injection-keys": "./src/injection-keys.ts"
  },
  "files": [
    "dist",
    "src/**/*.vue",
    "src/**/*.ts"
