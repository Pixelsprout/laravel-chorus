    "Offline first",
    "Local First",
    "Vue",
    "Vue 3",
    "composables"
  ],
  "devDependencies": {
    "@types/node": "^24.0.10",
    "typescript": "^5.8.3",
    "vue": "^3.5.0"
  },
  "dependencies": {
    "@pixelsprout/chorus-core": "@pixelsprout/chorus-core",
    "@laravel/echo-vue": "^2.1.7",
    "@vueuse/rxjs": "^12.8.2",
    "dexie": "^4.0.11"
  },
  "peerDependencies": {
    "vue": "^3.5.0"
  }
