{
  "name": "pixelsprout/laravel-chorus",
  "description": "Laravel Chorus is an event-sourcing based sync-engine designed to sync subsets of your database to your users' on device browser storage, to enable a low-latency web application.",
  "type": "library",
  "license": ["MIT"],
  "require": {
    "php": ">=8.2",
    "illuminate/contracts": "^10.47|^11.0|^12.0",
    "illuminate/http": "^10.47|^11.0|^12.0",
    "illuminate/support": "^10.47|^11.0|^12.0",
    "illuminate/database": "^10.47|^11.0|^12.0",
    "illuminate/redis": "^10.47|^11.0|^12.0",
    "laravel/prompts": "^0.3.0@dev"
  },
  "require-dev": {
    "orchestra/testbench-core": "^9.12.0 || ^10.4",
    "pestphp/pest": "^3.8.2",
    "predis/predis": "^2.0",
    "laravel/pint": "dev-main",
    "rector/rector": "dev-main",
    "laravel/framework": "^12.0",
    "mockery/mockery": "1.6.12"
  },
  "autoload": {
    "psr-4": {
      "Pixelsprout\\LaravelChorus\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Pixelsprout\\LaravelChorus\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Pixelsprout\\LaravelChorus\\Providers\\ChorusServiceProvider"
      ]
    }
  },
  "authors": [
    {
      "name": "Pixelsprout",
      "email": "support@pixelsprout.dev"
    }
  ],
  "minimum-stability": "dev",
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
